spring.application.name=21-batch-processing-app

spring.jpa.properties.hibernate.dialect = org.hibernate.dialect.MySQL8Dialect

spring.batch.job.enabled=false

spring.datasource.url=jdbc:mysql://localhost:3306/batchuserdb
spring.datasource.username=root
spring.datasource.password=root

spring.jpa.hibernate.ddl-auto=update
spring.jpa.show-sql=true


logging.level.root=info

logging.level.org.hibernate.springframework=info

#spring.sql.init.schema-locations=classpath:org/springframework/batch/core/schema-mysql.sql
